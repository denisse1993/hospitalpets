<hibernate-mapping package="hospitalpets.modelo">
    <class name="hospitalpets.modelo.Propietario">
        <id column="cedula" name="cedula">
        </id>
        <property column="nombre" name="nombre"/>
        <property column="apellido" name="apellido"/>
        <property column="direccion" name="direccion"/>
        <property column="telefono" name="telefono"/>
    </class>

</hibernate-mapping>